<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<VBox prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #3A2C63;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ce316.HelloController">
    <children>
        <SplitPane dividerPositions="0.5" prefHeight="391.0" prefWidth="659.0" VBox.vgrow="ALWAYS">
            <items>
                <TextFlow minWidth="200.0" prefHeight="373.0" prefWidth="300.0" style="-fx-background-color: #3A2C63;" textAlignment="JUSTIFY" />
                <VBox alignment="CENTER" minWidth="300.0" prefHeight="375.0" prefWidth="379.0" spacing="20" style="-fx-background-color: #4A3A7C;">
                    <children>
                        <Button fx:id="createNewProjectButton" mnemonicParsing="false" onAction="#setCreateNewProjectButton" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #FFA258;" text="Create New Project" />
                        <Button fx:id="openExistingProjectButton" mnemonicParsing="false" onAction="#setOpenExistingProjectButton" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #FFA258;" text="Edit Project" />
                  <Button mnemonicParsing="false" onAction="#setUploadStudentButton" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #FFA258;" text="Run Students' Files" />
                        <Button fx:id="assignmentReportsButton" mnemonicParsing="false" onAction="#setAssignmentReportsButton" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #FFA258;" text="Project Reports" />
                  <Button mnemonicParsing="false" onAction="#setConfigurations" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #FFA258;" text="Configurations" />
                  <Button mnemonicParsing="false" onAction="#setHelpButton" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #FFA258;" text="Help" />
                    </children>
                </VBox>
            </items>
        </SplitPane>
    </children>
</VBox>
