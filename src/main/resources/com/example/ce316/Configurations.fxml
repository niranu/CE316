<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #4A3A7C;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ce316.HelloController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="700.0">
         <children>
            <Label text="Configurations" textFill="WHITE">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox maxHeight="70.0" prefHeight="50.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Label prefHeight="26.0" prefWidth="157.0" text="Language Name:" textFill="WHITE" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="7.0" />
               </HBox.margin>
               <padding>
                  <Insets top="8.0" />
               </padding>
            </Label>
            <TextField prefHeight="26.0" prefWidth="536.0" promptText="Java, C, Python..." HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" maxHeight="70.0" prefHeight="50.0" prefWidth="200.0">
         <children>
            <Label prefHeight="26.0" prefWidth="156.0" text="Compiler/Interpreter Path:" textFill="WHITE" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="7.0" />
               </HBox.margin>
               <padding>
                  <Insets top="8.0" />
               </padding>
            </Label>
            <TextField prefHeight="26.0" prefWidth="526.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="59.0" prefWidth="589.0" VBox.vgrow="ALWAYS">
         <children>
            <Label prefHeight="28.0" prefWidth="164.0" text="Interpret / Compile:" textFill="WHITE">
               <padding>
                  <Insets left="7.0" top="10.0" />
               </padding>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
            <ComboBox prefHeight="26.0" prefWidth="229.0" style="-fx-background-color: #FFA258;">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="Interpreter" />
                        <String fx:value="Compiler" />
                    </FXCollections>
                </items>
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </ComboBox>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="526.0" VBox.vgrow="ALWAYS">
         <children>
            <Label prefHeight="28.0" prefWidth="164.0" text="Compilation Options:" textFill="WHITE" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets left="7.0" top="10.0" />
               </padding>
            </Label>
            <TextArea prefHeight="57.0" prefWidth="492.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextArea>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </HBox>
      <HBox layoutX="10.0" layoutY="302.0" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Label prefHeight="28.0" prefWidth="164.0" text="File Type:" textFill="WHITE" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets left="7.0" top="10.0" />
               </padding>
            </Label>
            <TextArea prefHeight="79.0" prefWidth="475.0" promptText=".c .main etc." HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextArea>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Label prefHeight="28.0" prefWidth="164.0" text="Dependencies:" textFill="WHITE" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets left="7.0" top="10.0" />
               </padding>
            </Label>
            <TextArea prefHeight="79.0" prefWidth="477.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextArea>
         </children>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0" VBox.vgrow="ALWAYS">
         <children>
            <Button mnemonicParsing="false" onAction="#setBackButton" prefHeight="25.0" prefWidth="131.0" style="-fx-background-color: #FFA258;" text="Back" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" style="-fx-background-color: #FFA258;" text="Save Configurations" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="110.0" style="-fx-background-color: #FFA258;" text="Export" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="109.0" style="-fx-background-color: #FFA258;" text="Import" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
